{
   "name": "@libragen/cli",
   "version": "0.5.0",
   "description": "Command-line interface for creating, managing, and querying libragen libraries",
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "bin": {
      "libragen": "./bin/run.js"
   },
   "files": [
      "dist",
      "bin"
   ],
   "scripts": {
      "build": "tsc",
      "test": "vitest run"
   },
   "oclif": {
      "bin": "libragen",
      "dirname": "libragen",
      "commands": "./dist/commands",
      "hooks": {
         "init": "./dist/hooks/init"
      },
      "plugins": [
         "@oclif/plugin-help",
         "@oclif/plugin-version",
         "@oclif/plugin-autocomplete",
         "@oclif/plugin-not-found"
      ],
      "topicSeparator": " ",
      "topics": {
         "collection": {
            "description": "Manage library collections"
         }
      },
      "additionalVersionFlags": [
         "-V",
         "--cli-version"
      ],
      "additionalHelpFlags": [
         "-h"
      ]
   },
   "keywords": [
      "cli",
      "rag",
      "libragen"
   ],
   "license": "MIT",
   "engines": {
      "node": ">=24.0.0"
   },
   "dependencies": {
      "@libragen/core": "^0.4.0",
      "@oclif/core": "4.8.0",
      "@oclif/plugin-autocomplete": "3.2.39",
      "@oclif/plugin-help": "6.2.36",
      "@oclif/plugin-not-found": "3.2.73",
      "@oclif/plugin-version": "2.2.36",
      "chalk": "5.6.2",
      "ora": "9.0.0",
      "semver": "^7.7.3",
      "tar": "7.4.3"
   },
   "devDependencies": {
      "@types/semver": "^7.7.1"
   }
}
